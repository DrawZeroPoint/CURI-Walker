<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Sequence name="Upstairs 15">
            <Sequence name="First step">
                <Action ID="ExecuteDualLegMove" left_ee_link="left_foot_sole" left_pose="0 -0.1 0.1 0 0 0 1" left_ref_link="left_foot_sole" name="Transfer CoM to left foot" right_ee_link="right_foot_sole" right_pose="0 -0.1 0.1 0 0 0 1" right_ref_link="right_foot_sole" server_name="/walker/dual_leg_control/move_to_ee_pose"/>
                <Sequence name="Swing right foot">
                    <Action ID="ExecuteRLegMove" ee_link="right_foot_sole" name="Right foot straight up 0.15" pose="0 0 0.15 0 0 0 1" ref_link="right_foot_sole" server_name="/walker/move_helper_right_leg/move_to_ee_pose"/>
                    <Action ID="ExecuteRLegMove" ee_link="right_foot_sole" name="Right foot forward 0.28" pose="0.28 0 0 0 0 0 1" ref_link="right_foot_sole" server_name="/walker/move_helper_right_leg/move_to_ee_pose"/>
                    <Action ID="ExecuteRLegMove" ee_link="right_foot_sole" name="Make right foot contact" pose="0 0 -0.05 0 0 0 1" ref_link="right_foot_sole" server_name="/walker/move_helper_right_leg/move_to_ee_pose"/>
                </Sequence>
            </Sequence>
            <Repeat num_cycles="2">
                <Sequence name="Intermediate steps">
                    <Action ID="ExecuteDualLegMove" left_ee_link="left_foot_sole" left_pose="-0.28 0.2 0 0 0 0 1" left_ref_link="left_foot_sole" name="Transfer CoM to right foot" right_ee_link="right_foot_sole" right_pose="-0.28 0.2 0 0 0 0 1" right_ref_link="right_foot_sole" server_name="/walker/dual_leg_control/move_to_ee_pose"/>
                    <Sequence name="Swing left foot">
                        <Action ID="ExecuteRLegMove" ee_link="right_foot_sole" name="Stand 0.15 on right foot" pose="0 0 -0.15 0 0 0 1" ref_link="right_foot_sole" server_name="/walker/move_helper_right_leg/move_to_ee_pose"/>
                        <Action ID="ExecuteLLegMove" ee_link="left_foot_sole" name="Left foot forward 0.28 up 0.15" pose="0.28 0 0.15 0 0 0 1" ref_link="left_foot_sole" server_name="/walker/move_helper_left_leg/move_to_ee_pose"/>
                        <Action ID="ExecuteLLegMove" ee_link="left_foot_sole" name="Left foot forward 0.28 down 0.05" pose="0.28 0 -0.05 0 0 0 1" ref_link="left_foot_sole" server_name="/walker/move_helper_left_leg/move_to_ee_pose"/>
                        <Action ID="ExecuteRLegMove" ee_link="right_foot_sole" name="Lower 0.05 on right leg" pose="0 0 0.05 0 0 0 1" ref_link="right_foot_sole" server_name="/walker/move_helper_right_leg/move_to_ee_pose"/>
                    </Sequence>
                    <Action ID="ExecuteDualLegMove" left_ee_link="left_foot_sole" left_pose="-0.28 -0.2 0 0 0 0 1" left_ref_link="left_foot_sole" name="Transfer CoM to left foot" right_ee_link="right_foot_sole" right_pose="-0.28 -0.2 0 0 0 0 1" right_ref_link="right_foot_sole" server_name="/walker/dual_leg_control/move_to_ee_pose"/>
                    <Sequence name="Swing right foot">
                        <Action ID="ExecuteLLegMove" ee_link="left_foot_sole" name="Stand 0.15 on left foot" pose="0 0 -0.15 0 0 0 1" ref_link="left_foot_sole" server_name="/walker/move_helper_left_leg/move_to_ee_pose"/>
                        <Action ID="ExecuteRLegMove" ee_link="right_foot_sole" name="Right foot forward 0.28 up 0.15" pose="0.28 0 0.15 0 0 0 1" ref_link="right_foot_sole" server_name="/walker/move_helper_right_leg/move_to_ee_pose"/>
                        <Action ID="ExecuteRLegMove" ee_link="right_foot_sole" name="Right foot forward 0.28 down 0.05" pose="0.28 0 -0.05 0 0 0 1" ref_link="right_foot_sole" server_name="/walker/move_helper_right_leg/move_to_ee_pose"/>
                        <Action ID="ExecuteLLegMove" ee_link="left_foot_sole" name="Lower 0,05 on left leg" pose="0 0 0.05 0 0 0 1" ref_link="left_foot_sole" server_name="/walker/move_helper_left_leg/move_to_ee_pose"/>
                    </Sequence>
                </Sequence>
            </Repeat>
            <Sequence name="Final step">
                <Action ID="ExecuteDualLegMove" left_ee_link="left_foot_sole" left_pose="-0.28 0.2 0 0 0 0 1" left_ref_link="left_foot_sole" right_ee_link="right_foot_sole" right_pose="-0.28 0.2 0 0 0 0 1" right_ref_link="right_foot_sole" server_name="/walker/dual_leg_control/move_to_ee_pose"/>
                <Sequence name="Swing left foot">
                    <Action ID="ExecuteRLegMove" ee_link="right_foot_sole" name="Up 0.15 on right leg" pose="0 0 -0.15 0 0 0 1" ref_link="right_foot_sole" server_name="/walker/move_helper_right_leg/move_to_ee_pose"/>
                    <Action ID="ExecuteLLegMove" ee_link="left_foot_sole" name="Left foot forward 0.28 up 0.15" pose="0.28 0 0.15 0 0 0 1" ref_link="left_foot_sole" server_name="/walker/move_helper_left_leg/move_to_ee_pose"/>
                    <Action ID="ExecuteLLegMove" ee_link="left_foot_sole" name="Left foot forward 0.28 down 0.05" pose="0.28 0 -0.05 0 0 0 1" ref_link="left_foot_sole" server_name="/walker/move_helper_left_leg/move_to_ee_pose"/>
                    <Action ID="ExecuteRLegMove" ee_link="right_foot_sole" name="Lower 0.05 on right leg" pose="0 0 0.05 0 0 0 1" ref_link="right_foot_sole" server_name="/walker/move_helper_right_leg/move_to_ee_pose"/>
                </Sequence>
                <Action ID="ExecuteDualLegMove" left_ee_link="left_foot_sole" left_pose="-0.28 -0.2 0 0 0 0 1" left_ref_link="left_foot_sole" name="Transfer CoM to left foot" right_ee_link="right_foot_sole" right_pose="-0.28 -0.2 0 0 0 0 1" right_ref_link="right_foot_sole" server_name="/walker/dual_leg_control/move_to_ee_pose"/>
                <Sequence name="Swing right foot">
                    <Action ID="ExecuteLLegMove" ee_link="left_foot_sole" name="Up 0.15 on left leg" pose="0 0 -0.15 0 0 0 1" ref_link="left_foot_sole" server_name="/walker/move_helper_left_leg/move_to_ee_pose"/>
                    <Action ID="ExecuteRLegMove" ee_link="right_foot_sole" name="Right foot forward 0.28 down 0.05" pose="0.28 0 -0.05 0 0 0 1" ref_link="right_foot_sole" server_name="/walker/move_helper_right_leg/move_to_ee_pose"/>
                </Sequence>
            </Sequence>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="EstimateAdjustPose">
            <input_port default="estimate_adjust_pose" name="service_name"/>
            <output_port default="{adjust_pose}" name="tgt_nav_pose"/>
        </Action>
        <Action ID="EstimateContactForce">
            <input_port default="x" name="direction">Could be x or z, x points to the hand back, z along lower arm</input_port>
            <input_port name="id">Could be left_wrist or right_wrist</input_port>
            <input_port default="0" name="max_force"/>
            <input_port default="0" name="min_force"/>
            <input_port default="estimate_contact_force" name="service_name"/>
        </Action>
        <Action ID="EstimateTargetPose">
            <output_port default="{rotation_compensate_pose}" name="compensate_pose"/>
            <input_port default="{detected_poses}" name="obj_poses"/>
            <input_port default="estimate_target_pose" name="service_name"/>
            <output_port default="{target_grasp_pose}" name="tgt_grasp_pose"/>
            <input_port default="{target_hover_pose}" name="tgt_hover_pose"/>
            <output_port default="{target_nav_pose}" name="tgt_nav_pose"/>
            <output_port default="{target_pre_grasp_pose}" name="tgt_pre_grasp_pose"/>
        </Action>
        <Action ID="ExecuteBalanceBase">
            <input_port default="/execute_balance_base" name="service_name"/>
        </Action>
        <Action ID="ExecuteDualArmJointStates">
            <input_port default="0 0 0 0 0 0 0" name="left_pose">Left arm joint states in degree</input_port>
            <input_port default="0" name="mirror">If mirror left arm joint trajectory to the right arm</input_port>
            <input_port default="0 0 0 0 0 0 0" name="right_pose">Right arm joint states in degree</input_port>
            <input_port default="/walker/dual_arm_control/move_to_joint_pose" name="server_name"/>
        </Action>
        <Action ID="ExecuteDualArmMove">
            <input_port default="0" name="do_cartesian">1: force the paths to follow straight lines</input_port>
            <input_port default="left_tcp" name="left_ee_link"/>
            <input_port name="left_pose">Left arm pose in position and orientation</input_port>
            <input_port default="base_link" name="left_ref_link">Could be base_link (absolute) or left_tcp (relative mode)</input_port>
            <input_port default="right_tcp" name="right_ee_link"/>
            <input_port name="right_pose">Right arm pose in position and orientation</input_port>
            <input_port default="base_link" name="right_ref_link">Could be base_link (absolute) or right_tcp (relative mode)</input_port>
            <input_port default="/walker/dual_arm_control/move_to_ee_pose" name="server_name"/>
        </Action>
        <Action ID="ExecuteDualLegJointStates">
            <input_port name="left_pose"/>
            <input_port name="right_pose"/>
            <input_port default="/walker/dual_leg_control/move_to_joint_pose" name="server_name"/>
        </Action>
        <Action ID="ExecuteDualLegMove">
            <input_port default="left_foot_sole" name="left_ee_link"/>
            <input_port name="left_pose"/>
            <input_port default="left_foot_sole" name="left_ref_link"/>
            <input_port default="right_foot_sole" name="right_ee_link"/>
            <input_port name="right_pose"/>
            <input_port default="right_foot_sole" name="right_ref_link"/>
            <input_port default="/walker/dual_leg_control/move_to_ee_pose" name="server_name"/>
        </Action>
        <Action ID="ExecuteHeadJointStates">
            <input_port default="0 0" name="joint_states"/>
            <input_port default="/walker/move_helper_head/move_to_joint_pose" name="server_name"/>
        </Action>
        <Action ID="ExecuteLArmJointStates">
            <input_port default="0 0 0 0 0 0 0" name="joint_states"/>
            <input_port default="/walker/move_helper_left_arm/move_to_joint_pose" name="server_name"/>
        </Action>
        <Action ID="ExecuteLArmMove">
            <input_port default="left_tcp" name="ee_link">The link to be controlled, default is left_tcp</input_port>
            <input_port name="pose"/>
            <input_port default="base_link" name="ref_link">Could be base_link or left_tcp</input_port>
            <input_port default="/walker/move_helper_left_arm/move_to_ee_pose" name="server_name"/>
        </Action>
        <Action ID="ExecuteLHandGrasp">
            <input_port default="/walker/hand_helper_left/grasp" name="server_name"/>
            <input_port name="type">0: Open hand, 1: Grasp can, 2: Grasp cart handle, 3: Grasp fridge handle</input_port>
        </Action>
        <Action ID="ExecuteLLegMove">
            <input_port default="left_foot_sole" name="ee_link"/>
            <input_port name="pose"/>
            <input_port default="left_foot_sole" name="ref_link"/>
            <input_port default="/walker/move_helper_left_leg/move_to_ee_pose" name="server_name"/>
        </Action>
        <Action ID="ExecuteLegMotion">
            <input_port name="joint_states">12 values in order: l_hip_y ; l_hip_r ; l_hip_p; l_knee ; l_ank_p ; l_ank_r</input_port>
            <input_port default="execute_leg_motion" name="service_name"/>
        </Action>
        <Action ID="ExecuteMoveBase">
            <input_port default="{target_nav_pose}" name="pose"/>
            <input_port default="execute_move_base" name="service_name"/>
        </Action>
        <Action ID="ExecuteMoveToAbsPose">
            <input_port name="pose"/>
            <input_port default="/move_base/move_to_abs_pos" name="service_name"/>
        </Action>
        <Action ID="ExecuteRArmJointStates">
            <input_port default="0 0 0 0 0 0 0" name="joint_states"/>
            <input_port default="/walker/move_helper_right_arm/move_to_joint_pose" name="server_name"/>
        </Action>
        <Action ID="ExecuteRArmMove">
            <input_port default="right_tcp" name="ee_link"/>
            <input_port name="pose"/>
            <input_port default="base_link" name="ref_link"/>
            <input_port default="/walker/move_helper_right_arm/move_to_ee_pose" name="server_name"/>
        </Action>
        <Action ID="ExecuteRHandGrasp">
            <input_port default="/walker/hand_helper_right/grasp" name="server_name"/>
            <input_port name="type">0: Open hand, 1: Grasp can, 2: Grasp cart handle, 3: Grasp fridge handle</input_port>
        </Action>
        <Action ID="ExecuteRLegMove">
            <input_port default="right_foot_sole" name="ee_link"/>
            <input_port name="pose"/>
            <input_port default="right_foot_sole" name="ref_link"/>
            <input_port default="/walker/move_helper_right_leg/move_to_ee_pose" name="server_name"/>
        </Action>
        <Action ID="ExecuteStabilizeBase">
            <input_port default="stabilize_base" name="service_name"/>
        </Action>
        <Action ID="ExecuteWalkCmd">
            <input_port default="start" name="cmd"/>
            <input_port default="execute_walk_cmd" name="service_name"/>
        </Action>
        <Action ID="SenseObjectPoses">
            <input_port default="box" name="goal_id"/>
            <output_port default="{detected_poses}" name="poses"/>
            <input_port default="/head/extract_object_on_top" name="service_name"/>
        </Action>
    </TreeNodesModel>
    <!-- ////////// -->
</root>

