<root>
    <TreeNodesModel>
        <Action ID="EstimateAdjustPose">
            <input_port name="service_name" default="estimate_adjust_pose"/>
            <output_port name="tgt_nav_pose" default="{adjust_pose}"/>
        </Action>
        <Action ID="EstimateContactForce">
            <input_port name="direction" default="x">Could be x or z, x points to the hand back, z along lower arm</input_port>
            <input_port name="id">Could be left_wrist or right_wrist</input_port>
            <input_port name="max_force" default="0"/>
            <input_port name="min_force" default="0"/>
            <input_port name="service_name" default="estimate_contact_force"/>
        </Action>
        <Action ID="EstimateTargetPose">
            <output_port name="compensate_pose" default="{rotation_compensate_pose}"/>
            <input_port name="obj_poses" default="{detected_poses}"/>
            <input_port name="service_name" default="estimate_target_pose"/>
            <output_port name="tgt_grasp_pose" default="{target_grasp_pose}"/>
            <input_port name="tgt_hover_pose" default="{target_hover_pose}"/>
            <output_port name="tgt_nav_pose" default="{target_nav_pose}"/>
            <output_port name="tgt_pre_grasp_pose" default="{target_pre_grasp_pose}"/>
        </Action>
        <Action ID="ExecuteHeadJointStates">
            <input_port name="joint_states" default="0 0"/>
            <input_port name="server_name" default="/walker/move_helper_head/move_to_joint_pose"/>
        </Action>
        <Action ID="ExecuteLArmJointStates">
            <input_port name="joint_states" default="0 0 0 0 0 0 0"/>
            <input_port name="server_name" default="/walker/move_helper_left_arm/move_to_joint_pose"/>
        </Action>
        <Action ID="ExecuteLArmMove">
            <input_port name="ee_link" default="left_tcp">The link to be controlled, default is left_tcp</input_port>
            <input_port name="pose"/>
            <input_port name="ref_link" default="base_link">Could be base_link or left_tcp</input_port>
            <input_port name="server_name" default="/walker/move_helper_left_arm/move_to_ee_pose"/>
        </Action>
        <Action ID="ExecuteLHandGrasp">
            <input_port name="server_name" default="/walker/hand_helper_left/grasp"/>
            <input_port name="type">0: Open hand, 1: Grasp can, 2: Grasp cart handle, 3: Grasp fridge handle</input_port>
        </Action>
        <Action ID="ExecuteMoveBase">
            <input_port name="pose" default="{target_nav_pose}"/>
            <input_port name="service_name" default="execute_move_base"/>
        </Action>
        <Action ID="ExecuteMoveToAbsPose">
            <input_port name="pose"/>
            <input_port name="service_name" default="/move_base/move_to_abs_pos"/>
        </Action>
        <Action ID="ExecuteRArmJointStates">
            <input_port name="joint_states" default="0 0 0 0 0 0 0"/>
            <input_port name="server_name" default="/walker/move_helper_right_arm/move_to_joint_pose"/>
        </Action>
        <Action ID="ExecuteRArmMove">
            <input_port name="ee_link" default="right_tcp"/>
            <input_port name="pose"/>
            <input_port name="ref_link" default="base_link"/>
            <input_port name="server_name" default="/walker/move_helper_right_arm/move_to_ee_pose"/>
        </Action>
        <Action ID="ExecuteRHandGrasp">
            <input_port name="server_name" default="/walker/hand_helper_right/grasp"/>
            <input_port name="type">0: Open hand, 1: Grasp can, 2: Grasp cart handle, 3: Grasp fridge handle</input_port>
        </Action>
        <Action ID="ExecuteStabilizeBase">
            <input_port name="service_name" default="stabilize_base"/>
        </Action>
        <Action ID="ExecuteWalkCmd">
            <input_port name="cmd" default="start"/>
            <input_port name="service_name" default="execute_walk_cmd"/>
        </Action>
        <Action ID="SenseObjectPoses">
            <input_port name="goal_id" default="box"/>
            <output_port name="poses" default="{detected_poses}"/>
            <input_port name="service_name" default="/head/extract_object_on_top"/>
        </Action>
    </TreeNodesModel>
</root>

